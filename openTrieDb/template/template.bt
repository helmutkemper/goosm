struct FILE {    struct LINE {		uint length<bgcolor=cLtGreen>;		uint64 nextAddress<bgcolor=cLtBlue>;		struct SLOT {			byte piece[4]<bgcolor=cLtRed>;			int64 addressLine<bgcolor=cLtYellow>;			byte flag<bgcolor=cLtGray, comment=FlagName>;		} dataSet[64]<format=hex, fgcolor=cBlack, bgcolor=cLtGray>;    	} lineData[1];} file;string FlagName(byte &flag) {    if( flag==0 ){ return "flagNotSet"; }    if( flag==1 ){ return "flagContinue"; }    if( flag==2 ){ return "flagComplete"; }    if( flag==3 ){ return "flagContComp"; }    if( flag==4 ){ return "flagItSelf"; }       return "";}